    "synchronize",
    "multiple",
    "bind",
    "mirror",
    "two-way"
  ],
  "author": "VueFrag",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuefrag/vue-sync-refs.git"
  },
  "bugs": {
    "url": "https://github.com/vuefrag/vue-sync-refs/issues"
  },
  "homepage": "https://github.com/vuefrag/vue-sync-refs#readme",
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "unbuild": "^2.0.0",
    "typescript": "^5.0.0",
    "vue": "^3.4.0"
  }
}